{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "publicPath": "/",
  "outputPath": "./dist",
  "extraBabelPlugins": [
    "transform-runtime",
    [
      "import",
      {
        "libraryName": "antd",
        "libarayDirectory": "lib",
        "style": true
      }
    ]
  ],
  "extraPostCSSPlugins": [],
  "autoprefixer": {
    "browsers": [
      "> 5%",
      "last 2 versions",
      "iOS >= 8",
      "Android >= 4"
    ]
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:7001",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api": ""
      }
    }
  },
  "externals": null,
  "multipage": false,
  "define": null,
  "theme": {
    "primary-color": "#2ca6cb"
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  }
}
